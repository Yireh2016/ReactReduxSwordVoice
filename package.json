{
  "name": "my-ssr-app",
  "version": "1.0.0",
  "description": "React SSR Redux Blog App",
  "scripts": {
    "dev:build-server": "webpack --env.ENVIRONMENT=development --config webpack.config.js --mode=development -w     ",
    "dev:start": "nodemon server/server.js",
    "dev": "concurrently \"npm:dev:build-server\" \"npm:dev:start\" ",
    "build": "webpack --env.ENVIRONMENT=production --config webpack.config.js --mode=production",
    "start": "node server/server.js",
    "build-start": "npm run build && npm run start",
    "build-prod-local": "webpack --env.ENVIRONMENT=production --config webpack.config.js --mode=production",
    "start-prod-local": "node server/server.js",
    "start-prod": "npm run build-prod-local && npm run start-prod-local"
  },
